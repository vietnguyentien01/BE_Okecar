{
    "name": "oke_car",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "license": "",
    "scripts": {
        "prebuild": "rimraf dist",
        "copy:packagejson": "cpx './package.json' 'dist'",
        "copy:cert": "cpx './src/configs/cert/**' 'dist/configs/cert'",
        "build": "nest build && npm run copy:packagejson && npm run copy:cert",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "npm run copy:packagejson && nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main.js",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "pre-commit": "lint-staged",
        "precommit": "lint-staged",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare": "husky install"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,js}": [
            "pretty-quick --staged",
            "eslint --fix",
            "git add"
        ]
    },
    "dependencies": {
        "@elastic/elasticsearch": "7.17.0",
        "@metamask/eth-sig-util": "4.0.1",
        "@nestjs-modules/mailer": "^1.8.1",
        "@nestjs/bull": "0.6.1",
        "@nestjs/common": "8.4.7",
        "@nestjs/core": "8.4.7",
        "@nestjs/elasticsearch": "^9.0.0",
        "@nestjs/jwt": "8.0.1",
        "@nestjs/mongoose": "9.0.1",
        "@nestjs/platform-express": "8.4.7",
        "@nestjs/schedule": "2.0.1",
        "@nestjs/terminus": "8.1.0",
        "@nestjs/throttler": "2.0.1",
        "@sendgrid/mail": "7.7.0",
        "@trivago/prettier-plugin-sort-imports": "^3.3.0",
        "@trustgem/search-base-module": "^0.0.10",
        "@types/aws-sdk": "2.7.0",
        "@types/cache-manager": "3.4.0",
        "@types/mongodb": "3.6.20",
        "@types/multer": "1.4.5",
        "apollo-server-express": "2.25.4",
        "aws-sdk": "2.831.0",
        "axios": "^1.2.0",
        "bcrypt": "^5.0.1",
        "bull": "4.8.5",
        "cache-manager": "3.6.3",
        "cache-manager-redis-store": "2.0.0",
        "cheerio": "^1.0.0-rc.12",
        "class-transformer": "0.2.3",
        "class-validator": "0.12.2",
        "cpx": "1.5.0",
        "csv-parse": "5.3.0",
        "dotenv": "16.0.1",
        "fico-web3": "1.7.1-rc.8",
        "handlebars": "^4.7.7",
        "mongoose": "6.0.15",
        "node-base64-image": "2.0.4",
        "node-telegram-bot-api": "0.59.0",
        "nodemailer": "^6.8.0",
        "redis": "4.1.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.4.0",
        "sharp": "0.30.7",
        "ts-node": "8.10.2",
        "uuid": "8.3.2",
        "voucher-code-generator": "1.3.0",
        "web3": "1.7.4"
    },
    "devDependencies": {
        "@nestjs/cli": "8.2.8",
        "@nestjs/schematics": "8.0.11",
        "@nestjs/testing": "8.4.7",
        "@types/bcrypt": "^5.0.0",
        "@types/bull": "3.15.9",
        "@types/express": "4.17.13",
        "@types/jest": "25.1.2",
        "@types/node": "13.13.52",
        "@types/sharp": "^0.30.4",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.30.0",
        "@typescript-eslint/parser": "5.30.0",
        "eslint": "8.0.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.1.0",
        "eslint-plugin-unused-imports": "2.0.0",
        "husky": "8.0.1",
        "jest": "24.9.0",
        "prettier": "2.7.1",
        "supertest": "4.0.2",
        "ts-loader": "6.2.2",
        "tsconfig-paths": "3.14.1",
        "typescript": "4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
